{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/generate-meal-plans",
      "schedule": "* 5 * * 6"
    },
    {
      "path": "/api/cron/weekly-meal-plans",
      "schedule": "0 6 * * 6"
    }
  ]
} 